library(tidyverse)
library(janitor)
anolis <- read.csv("data/anolis_dat.csv")
View(anolis)
glimpse(anolis)
anolis <- clean_names(anolis)
anolis <- anolis %>%
mutate(habitat=as.factor(habitat),
island=as.factor(island))
View(anolis)
glimpse(anolis)
anolis$island
anolis %>%
distinct(island)
anolis %>%
ggplot(mapping=aes(x=island))+
geom_bar()
anolis %>%
ggplot(mapping=aes(x=island))+
geom_bar(fill=island)+
labs(title="Anole species sampled from each island",x="Island",y="Number of anole species sampled")
anolis %>%
ggplot(mapping=aes(x=island))+
geom_bar()+
labs(title="Anole species sampled from each island",x="Island",y="Number of anole species sampled")
anolis %>%
ggplot(mapping=aes(x=island))+
geom_bar(fill="island")+
labs(title="Anole species sampled from each island",x="Island",y="Number of anole species sampled")
anolis %>%
ggplot(mapping=aes(x=island))+
geom_bar(fill="pink")+
labs(title="Anole species sampled from each island",x="Island",y="Number of anole species sampled")
anolis %>%
ggplot(mapping=aes(x=island))+
geom_bar()+
labs(title="Anole species sampled from each island",x="Island",y="Number of anole species sampled")
anolis %>%
distinct(habitat)
anolis %>%
ggplot(mapping=aes(x=habitat))+
geom_bar()+
labs(title="Number of anole species sampled from each habitat",x="Habitat type",y="Number of anole species sampled")
anolis %>%
ggplot(mapping=aes(x=habitat,y=hindlimb_length_mm))+
geom_boxplot()+
labs(title="Hindlimb length in each habitat type",x="Habitat type",y="Hindlimb length (mm)")
anolis %>%
mutate(ratio_of_hindlimb_to_body=hindlimb_length_mm/body_length_mm, ratio_of_tail_to_body=tail_length_mm/body_length_mm)
anolis <- anolis %>%
mutate(ratio_of_hindlimb_to_body=hindlimb_length_mm/body_length_mm, ratio_of_tail_to_body=tail_length_mm/body_length_mm)
anolis %>%
ggplot(mapping=aes(x=habitat,y=ratio_of_hindlimb_to_body))+
geom_boxplot()+
labs(title="Ratio of hindlimb length to body length in each habitat type",x="Habitat type",y="Hindlimb length (mm)/body length (mm)")
anolis %>%
ggplot(mapping=aes(x=body_length_mm,y=tail_length_mm))+
geom_point(color=habitat)+
geom_smooth(method=lm)+
labs(title="Body length vs tail length",x="Body length (mm)",y="Tail length (mm)")
anolis %>%
ggplot(mapping=aes(x=body_length_mm,y=tail_length_mm))+
geom_point(color=anolis$habitat)+
geom_smooth(method=lm)+
labs(title="Body length vs tail length",x="Body length (mm)",y="Tail length (mm)")
anolis %>%
ggplot(mapping=aes(x=body_length_mm,y=tail_length_mm,color=species))+
geom_point()+
geom_smooth(method=lm)+
labs(title="Body length vs tail length",x="Body length (mm)",y="Tail length (mm)")
anolis %>%
ggplot(mapping=aes(x=body_length_mm,y=tail_length_mm,color=habitat))+
geom_point()+
geom_smooth(method=lm)+
labs(title="Body length vs tail length",x="Body length (mm)",y="Tail length (mm)")
anolis %>%
ggplot(mapping=aes(x=body_length_mm,y=tail_length_mm))+
geom_point(mapping=aes(color=habitat))+
geom_smooth(method=lm)+
labs(title="Body length vs tail length",x="Body length (mm)",y="Tail length (mm)")
anolis %>%
ggplot(mapping=aes(x=body_length_mm,y=tail_length_mm))+
geom_point(mapping=aes(color=habitat))+
geom_smooth(method=lm, color="black")+
labs(title="Body length vs tail length",x="Body length (mm)",y="Tail length (mm)")
hab_and_lamellae <- anolis %>%
