library("tidyverse")
options(scipen=999) #turn off scientific notation
homerange <- read.csv("data/Tamburelloetal_HomeRangeDatabase.csv")
view(homerange)
glimpse(homerange)
names(homerange)
ggplot(data=homerange, mapping=aes(x=taxon,y=N))+
geom_bar()
ggplot(data=homerange, mapping=aes(x=class))+
geom_bar()
ggplot(data=homerange, mapping=aes(x=class))+
geom_bar()+
labs(title="Number in each class", x="Class",y="Count")
ggplot(data=homerange, mapping=aes(x=class, color=class))+
geom_bar()+
labs(title="Number in each class", x="Class",y="Count")
ggplot(data=homerange, mapping=aes(x=class, fill=class))+
geom_bar()+
labs(title="Number in each class", x="Class",y="Count")
ggplot(data=homerange, mapping=aes(x=trophic.guild, fill=trophic.guild))+
geom_bar()+
labs(title="Herbivores vs Carnivores", x="Trophic Guild")
homerange %>%
select(common.name, order, mean.hra.m2, mean.mass.g) %>%
filter(mean.hra.m2>1000000) %>%
arrange(desc(mean.hra.m2)) %>%
slice_max(mean.hra.m2, n=10)
homerange %>%
select(species, mean.hra.m2) %>%
slice_max(mean.hra.m2, n=1)
homerange %>%
select(species, mean.hra.m2) %>%
slice_min(mean.hra.m2, n=1)
homerange %>%
filter(class=="mammalia", trophic.guild=="carnivore") %>%
slice_max(mean.hra.m2, n=10)
