library(tidyverse)
library(janitor)
sydneybeaches <- read.csv("data/sydneybeaches.csv")
sydneybeaches <- clean_names(sydneybeaches)
sydneybeaches_long <- sydneybeaches %>%
select(site,date,enterococci_cfu_100ml)
sydneybeaches_wide <- pivot_wider(sydneybeaches_long, names_from = date, values_from = enterococci_cfu_100ml)
pivot_longer(sydneybeaches_wide, cols = !"site", names_to = "date", values_to = "enterococci_cfu_100ml")
sydneybeaches_long <- separate(sydneybeaches_long, col="date", into = c("day","month","year"), sep = "/")
sydneybeaches_long <- sydneybeaches_long %>%
group_by(site,year) %>%
summarise(mean_enterococci_cfu_100ml= mean(enterococci_cfu_100ml, na.rm=T))
sydneybeaches_long <- pivot_wider(sydneybeaches_long, names_from = site, values_from = mean_enterococci_cfu_100ml)
sydneybeaches_long %>%
filter(year=="2013")
separate(sydneybeaches, col="date", into = c("day","month","year"), sep = "/") %>%
select(site,year,enterococci_cfu_100ml) %>%
ggplot(mapping=aes(x=site,y=enterococci_cfu_100ml))+
geom_boxplot()+
coord_cartesian(ylim=c(0, 2600))+
labs(title="Pollution level of each beach")
